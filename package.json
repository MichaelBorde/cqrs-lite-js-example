    "axios": "0.18.0",
    "jest": "23.6.0",
    "nodemon": "1.18.4",
    "npm-run-all": "4.1.3",
    "prettier": "1.14.3",
    "rimraf": "2.6.2",
    "ts-jest": "23.10.1",

{
  "name": "crqs-lite-js-example",
  "version": "0.0.1",
  "description": "",
  "files": [
    "build"
  ],
  "scripts": {
    "tslint": "tslint --fix 'lib/**/*.ts'",
    "jest:unit": "jest --config ./dev/jest.config.js",
    "jest:e2e": "jest --config ./dev/e2e/jest.config.e2e.js --runInBand",
    "test:e2e": "run-s build db:migrate:e2e jest:e2e",
    "testw": "jest --config ./dev/jest.config.js --watch",
    "prettier": "prettier --write '**/*.{js,json,md,ts}'",
    "compile": "tsc",
    "test": "run-s compile jest:unit tslint",
    "prebuild": "rimraf ./build",
    "build": "tsc -p ./tsconfig.prod.json",
    "db:migrate": "knex migrate:latest",
    "db:migrate:e2e": "node ./dev/e2e/migrate",
    "start": "node ./build/server.js",
    "ts-start": "ts-node ./lib/server.ts",
    "live": "nodemon --config ./dev/nodemon.json"
  },
  "author": "Michael Borde",
